sudo nano /etc/apt/source.list
sudo apt update -y && sudo apt upgrade -y

#install default packages
sudo apt install -y bat build-essential cargo cmake curl ffmpeg fzf g++ gettext git i3 i3blocks lightdm neovim network-manager ninja-build nitrogen nm-tray openssh-client openssh-server picom pkg-config polybar pulseaudio python3 snapd stow tealdeer wget x11-xserver-utils zoxide zsh
#sudo apt install -y freecad gimp
sudo snap install brave
sudo curl -fsSL https://raw.githubusercontent.com/Homebrew/install/HEAD/install.sh | /bin/bash

mkdir ~/repos
cd ~/repos
git clone  https://github.com/nvim-lua/kickstart.nvim.git ~/.config/nvim/
git clone --depth=1 https://github.com/romkatv/powerlevel10k.git
git clone https://github.com/zsh-users/zsh-syntax-highlighting.git
git clone https://github.com/zsh-users/zsh-autosuggestions
git clone https://github.com/sxyazi/yazi.git
cd yazi
cargo build --release --locked

chsh -s $(which zsh)

cd ~/
git clone https://github.com/corbgarza/dotfiles
scp corbing@192.168.1.9:~/.ssh/id_github ~/.ssh/
ssh-agent
ssh-add ~/.ssh/id_github
git remote set-url --push origin git@github.com:corbgarza/dotfiles
git config core.editor nvim
stow alacritty i3 myscripts p10k picom zsh

sudo cp ~/dotfiles/fonts/* /usr/local/share/fonts/
cp ~/dotfiles/animeLauren.png ~/Pictures/
